<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- The launcher theme. It sets the main window background to the launch_screen drawable -->
  <style name="Theme.AppCompat.Light.NoActionBar.Launcher">
    <item name="android:windowBackground">@drawable/ism_splash_screen</item>

  </style>
  <style name="TransparentBackgroundDialogStyle" parent="Theme.Design.Light.BottomSheetDialog">
    <item name="android:background">@android:color/transparent</item>
    <item name="android:colorBackground">@android:color/transparent</item>
  </style>

  <declare-styleable name="TouchImageView">
    <attr name="zoom_enabled" format="boolean" />
  </declare-styleable>
</resources>

